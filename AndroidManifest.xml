<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="1" android:versionName="2.0" package="de.djnilse.gps_countryoptimizer"
  xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto">

<uses-sdk android:minSdkVersion="10" android:targetSdkVersion="17" android:maxSdkVersion="17"/>

    <uses-permission android:name="android.permission.ACCESS_SUPERUSER" />
    <application
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:allowBackup="true"
        android:allowClearUserData="true"
        android:hardwareAccelerated="true"
		android:theme="@style/Theme.Sherlock.Light.DarkActionBar">
        <activity
            android:label="@string/app_name"
            android:name="de.djnilse.gps_countryoptimizer.GPSCountryChangerInterface">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="de.djnilse.gps_countryoptimizer.About" />
    </application>
</manifest>
